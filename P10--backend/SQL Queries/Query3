SELECT *
	FROM public.boundaries_1000m
	WHERE ST_INTERSECTS(ST_SetSRID( ST_Point(2.027793771968425, 52.93105121401569), 4326),boundary)
	
SELECT boundary, ST_AsText(boundary) from boundaries_1000m
limit 2

ALTER TABLE fact_ais_clean_v2 ADD COLUMN cell_id BIGINT;
ALTER TABLE fact_ais_clean_v2 ADD CONSTRAINT fk_cell_id FOREIGN KEY (cell_id) REFERENCES dim_cell (cell_id);

UPDATE fact_ais
SET cell_id = SELECT cell_id from dim_cell where ST_INTERSECTS(boundary_50m_id, coordinate) limit 1